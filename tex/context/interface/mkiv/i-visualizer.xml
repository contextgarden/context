    <cd:command name="showbreakpoints" level="document" file="trac-brk.mkxl">
        <cd:arguments>
            <cd:assignments list="yes" optional="yes">
                <cd:inherit name="setupcolumns"/>
            </cd:assignments>
        </cd:arguments>
    </cd:command>

    <cd:command name="showbreakpoints" type="environment" level="document" file="trac-brk.mkxl">
        <cd:arguments>
            <cd:assignments list="yes" optional="yes">
                <cd:parameter name="option">
                    <cd:constant type="margin"/>
                    <cd:constant type="simple"/>
                </cd:parameter>
                <!-- I'm unable to determine what arguments the list key accepts -->
                <cd:parameter name="list" list="yes">
                    <cd:constant type="cd:text"/>
                </cd:parameter>
                <cd:parameter name="offset">
                    <cd:constant type="cd:dimension"/>
                </cd:parameter>
            </cd:assignments>
        </cd:arguments>
    </cd:command>

