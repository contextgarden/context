%D \module
%D   [       file=type-imp-penstander,
%D        version=2025.11.12,
%D          title=\CONTEXT\ Typescript Macros,
%D       subtitle=Pennstander fonts,
%D         author={Julius Ross, Hans Hagen, Mikael Sundqvist}, % adapted from original
